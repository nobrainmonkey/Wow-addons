["custom_type"] = "status",
["check"] = "event",
["custom"] = "function(event, ...)\n    if event == \"LIQUID_CUSTOM_EVENT\" then\n        local id, sound = ...\n        \n        if id ~= aura_env.id then return end\n        \n        LiquidWeakAuras:PlaySound(sound, aura_env.ttsVoice)\n    elseif event == \"UNIT_SPELLCAST_SUCCEEDED\" then\n        local _, castGUID, spellID = ...\n        \n        if not castGUID then return end\n        if spellID ~= 1225319 then return end -- Galactic Smash\n        \n        aura_env.set = aura_env.set + 1\n        \n        local queuedNumber = 1\n        local id = aura_env.id\n        \n        for _, moveNumber in ipairs(aura_env.moves[aura_env.set]) do\n            for wave = queuedNumber, moveNumber do\n                local number = moveNumber - wave\n                local sound = number == 0 and \"move\" or number\n                \n                C_Timer.After(4.2 + wave * 2, function() WeakAuras.ScanEvents(\"LIQUID_CUSTOM_EVENT\", id, sound) end)\n            end\n            \n            queuedNumber = moveNumber + 1\n        end\n    elseif event == \"ENCOUNTER_START\" then\n        aura_env.set = 0\n        \n        aura_env:ReadNote()\n    end\nend",
