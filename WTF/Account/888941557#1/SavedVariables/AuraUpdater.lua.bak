["custom_type"] = "stateupdate",
["customVariables"] = "{\n    assignmentType = {\n        display = \"Assignment type\",\n        type = \"select\",\n        values = {\n            SPAWN = \"Spawn\",\n            BREAK = \"Break\"\n        }\n    }\n}",
