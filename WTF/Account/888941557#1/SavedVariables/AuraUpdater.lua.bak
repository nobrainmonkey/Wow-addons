["custom"] = "function(allstates, event, ...)\n    if event == \"OPTIONS\" then\n        allstates[\"\"] = {\n            show = true,\n            changed = true,\n            progressType = \"timed\",\n            duration = 5.6,\n            expirationTime = GetTime() + 5.6,\n            autoHide = true\n        }\n    elseif event == \"UNIT_SPELLCAST_SUCCEEDED\" then\n        local _, castGUID, spellID = ...\n        \n        if not castGUID then return end\n        \n        if spellID == 1232568 then -- Metamorphosis (Ilyssa Darksorrow)\n            aura_env.metaCount = aura_env.metaCount + 1\n            \n            if aura_env.metaCount > 2 then\n                allstates[\"\"] = {\n                    show = true,\n                    changed = true,\n                    progressType = \"timed\",\n                    duration = 5.6,\n                    expirationTime = GetTime() + 5.6,\n                    autoHide = true\n                }\n                \n                return true\n            end\n        elseif spellID == 1227117 then -- Fel Devastation\n            aura_env.felDevCount = 0\n            \n            if aura_env.felDevCount ~= 3 then -- 3rd one is the last Fel Devastation in 3rd intermission\n                allstates[\"\"] = {\n                    show = true,\n                    changed = true,\n                    progressType = \"timed\",\n                    duration = 8,\n                    expirationTime = GetTime() + 8,\n                    autoHide = true\n                }\n                \n                return true\n            end\n        end\n    elseif event == \"ENCOUNTER_START\" then\n        aura_env.metaCount = 0\n        aura_env.felDevCount = 0\n    end    \nend",
