["custom"] = "aura_env.noteIdentifier = \"Wave\"\naura_env.castSpellID = 1227226 -- Writhing Wave\naura_env.vulnSpellID = 1227163 -- Writhing Wave\naura_env.playersPerSoak = nil -- Leave as nil to assign everyone listed\n\n-- If set, simply splits the raid in two when no note is supplied\n-- If not set (and no note is supplied), the aura does nothing\naura_env.autoSplit = true\naura_env.includeTanks = false -- Whether tanks should be included in the auto split",
