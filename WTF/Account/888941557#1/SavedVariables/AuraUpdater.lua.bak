["type"] = "custom",
["custom_type"] = "stateupdate",
["customVariables"] = "{\n    additionalProgress = 1,\n    expirationTime = true,\n    lastSet = {\n        display = \"Last set\",\n        type = \"bool\"\n    },\n    overlap = {\n        display = \"May overlap\",\n        type = \"bool\"\n    }\n}",
