["custom"] = "function(event, ...)\n    if not aura_env.initialized then return end\n    \n    if event == \"ENCOUNTER_START\" then\n        LiquidWeakAuras:BroadcastSpec()\n        \n        -- For when we iterate spec IDs on ENCOUNTER_START\n        -- Doing it on this event instead should guarantee that spec IDs are known\n        local encounterID, encounterName, difficultyID, groupSize = ...\n        \n        C_Timer.After(1, function() WeakAuras.ScanEvents(\"LIQUID_ENCOUNTER_START\", encounterID, encounterName, difficultyID, groupSize) end)\n    elseif event == \"GROUP_ROSTER_UPDATE\" then\n        aura_env.GUIDToUnit = {}\n        \n        for unit in WA_IterateGroupMembers() do\n            aura_env.GUIDToUnit[UnitGUID(unit)] = unit\n        end\n    elseif event == \"LIQUID_WA_SPEC\" then\n        local GUID, specID = ...\n        \n        aura_env.storedUnits[GUID] = specID\n    elseif event == \"LIQUID_PRIVATE_AURA_MACRO\" then\n        if InCombatLockdown() then return end -- Only show test messages out of combat\n        \n        print(\"|cff00ff00Liquid macro pressed!\")\n    elseif event == \"PLAYER_ALIVE\" then\n        LiquidWeakAuras:Broadcast(\"LIQUID_UNIT_ALIVE\", WeakAuras.myGUID)\n    elseif event == \"LIQUID_WA_SPEC_REQUEST\" or event == \"PLAYER_SPECIALIZATION_CHANGED\" then\n        LiquidWeakAuras:BroadcastSpec()\n    elseif event == \"STATUS\" then\n        aura_env.GUIDToUnit = {}\n        \n        for unit in WA_IterateGroupMembers() do\n            aura_env.GUIDToUnit[UnitGUID(unit)] = unit\n        end\n        \n        LiquidWeakAuras:RequestSpec()\n    end\nend",
