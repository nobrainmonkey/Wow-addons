["customVariables"] = "{\n    castType = {\n        display = \"Cast type\",\n        type = \"select\",\n        values = {\n            CAST = \"Cast\",\n            CHANNEL = \"Channel\"\n        }\n    }\n}",
["event"] = "Health",
