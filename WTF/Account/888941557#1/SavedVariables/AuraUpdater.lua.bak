["init"] = {
["do_custom"] = true,
["custom"] = "if not aura_env.saved then aura_env.saved = {} end\n\nlocal ttsVoices = {\n    [1] = \"Davis\",\n    [2] = \"Maise\",\n    [3] = \"Sara\",\n    [4] = \"Steffan\"\n}\n\naura_env.ttsVoice = ttsVoices[aura_env.config.ttsVoice]\n\nif aura_env.saved.ttsVoice and aura_env.config.ttsVoice ~= aura_env.saved.ttsVoice then\n    LiquidWeakAuras:PlaySound(2, aura_env.ttsVoice)\n    C_Timer.After(1, function() LiquidWeakAuras:PlaySound(1, aura_env.ttsVoice) end)\n    C_Timer.After(2, function() LiquidWeakAuras:PlaySound(\"move\", aura_env.ttsVoice) end)\nend\n\naura_env.saved.ttsVoice = aura_env.config.ttsVoice\n\nfunction aura_env:ReadNote()\n    aura_env.moves = {{}, {}, {}}\n    \n    if aura_env.config.override then\n        for set = 1, 3 do\n            for move = 1, 3 do\n                local key = string.format(\"set%d_%d\", set, move)\n                local number = aura_env.config[key]\n                \n                number = number - 1 -- Index 1 is \"no move\", index 2 is move after 1st, etc.\n                \n                if number > 0 then\n                    tInsertUnique(aura_env.moves[set], number)\n                end\n            end\n        end\n    else\n        for lineCount, line in LiquidWeakAuras:IterateNoteAssignment(\"Spikes\") do\n            if not aura_env.moves[lineCount] then break end -- Don't assign more than 3 sets\n            \n            local numbers = strsplittable(\" \", line)\n            \n            for _, number in ipairs(numbers) do\n                number = tonumber(number)\n                \n                if number and number <= 12 then\n                    tInsertUnique(aura_env.moves[lineCount], number)\n                end\n            end\n        end\n    end\n    \n    for _, moves in ipairs(aura_env.moves) do\n        table.sort(moves)\n    end\nend",
},
},
